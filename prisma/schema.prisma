generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model App {
  id   Int    @id
  name String
  icon String
  Item Item[]
}

model Item {
  id            Int      @id
  name          String
  hash_name     String
  icon_url      String
  app_id        Int
  lastUpdatedAt DateTime @default(now())
  app           App      @relation(fields: [app_id], references: [id])
}
